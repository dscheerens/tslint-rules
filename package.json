{
  "name": "@dscheerens/tslint-rules",
  "version": "0.2.1",
  "description": "Extra TSLint rules",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build/dist && tsc -p src && cpx package.json build/dist",
    "build-docs": "rimraf docs && ts-node ./scripts/build-docs.ts",
    "lint": "tslint -t stylish -c tslint.json 'src/**/*.ts'",
    "test": "jasmine-ts \"test/**/*.spec.ts\"",
    "prepare-release": "npm run build && cpx README.md build/dist",
    "release": "npm publish build/dist"
  },
  "keywords": [
    "tslint",
    "extra",
    "extension",
    "rule",
    "rules"
  ],
  "author": "Daan Scheerens",
  "license": "MIT",
  "peerDependencies": {
    "tslint": "^5.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^8.9.4",
    "cpx": "^1.5.0",
    "jasmine": "^3.0.0",
    "jasmine-ts": "^0.2.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "ts-node": "^5.0.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tsutils": "^2.21.2",
    "typescript": "^2.7.2"
  }
}
