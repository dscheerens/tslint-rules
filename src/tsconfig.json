    "compilerOptions": {
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,

        "module": "commonjs",
        "importHelpers": true,
        "declaration": true,
        "sourceMap": false,
        "target": "es5",
        "lib": [
            "es6"
        ],
        "outDir": "../build/dist"
    }
}
